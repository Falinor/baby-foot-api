{
  "name": "babyfoot-api",
  "version": "0.0.0",
  "main": "src",
  "private": true,
  "scripts": {
    "start": "node .",
    "test": "ava",
    "test:unit": "npm test \"src/**/!(index).test.js\"",
    "test:integration": "npm test \"src/**/index.test.js\"",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "dev": "nodemon -i \"*.test.js\" .",
    "lint": "eslint src",
    "docs": "apidoc -i src -o docs && apidoc-markdown -p docs -o DOCS.md",
    "postdocs": "opn docs/index.html"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "source": [
      "src/**/!(*.test.js)"
    ],
    "failFast": true,
    "failWithoutAssertions": false,
    "concurrency": 5,
    "timeout": "5s",
    "tap": false,
    "powerAssert": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "apidoc-markdown": "^0.2.0",
    "ava": "^0.19.1",
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mockgoose": "^7.3.1",
    "nock": "^9.0.13",
    "nodemon": "^1.11.0",
    "opn-cli": "^3.1.0",
    "sinon": "^2.2.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "bodymen": "^1.0.3",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "lodash": "^4.17.4",
    "mongoose": "^4.9.8",
    "mongoose-create-unique": "^0.4.4",
    "mongoose-keywords": "^0.3.2",
    "morgan": "^1.8.1",
    "querymen": "^2.1.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  }
}
